curl -H 'Content-Type: application/json' -H 'Accept: application/json' -X POST http://www.udeal.io/api/v1/users.json -d '{"api_v1_user": {"email":"teststuff@test15.com", "username": "testing6000", "password": "12345678"}}'

curl -H 'Content-Type: application/json' -H 'Accept: application/json' -X POST http://www.udeal.io/api/v1/businesses.json -d '{"api_v1_business": {"email":"teststuff@google.com", "password": "12345678"}}'

