curl -i -H "Accept: application/json" -H "Content-type: application/json" -H "X-User-Email: teststuff@test15.com" -H "X-User-Token: Zkoj5dYfmC5tswcyzxn9" -X GET www.udeal.io/api/v1/deals

curl -i -H "Accept: application/json" -H "Content-type: application/json" -H "X-User-Email: teststuff@test15.com" -H "X-User-Token: Zkoj5dYfmC5tswcyzxn9" -X DELETE www.udeal.io/api/v1/users/sign_out.json

