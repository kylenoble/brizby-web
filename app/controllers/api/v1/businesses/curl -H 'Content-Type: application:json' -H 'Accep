curl -i -H "Accept: application/json" -H "Content-type: application/json" -H "X-User-Email: teststuff@google.com" -H "X-User-Token: AdguJNsZgxRZs8zspsqF" -X DELETE www.udeal.io/api/v1/businesses/sign_out.json

curl -i -H "Accept: application/json" -H "Content-type: application/json" -H "X-User-Email: teststuff@google.com" -H "X-User-Token: AdguJNsZgxRZs8zspsqF" -X GET www.udeal.io/api/v1/deals